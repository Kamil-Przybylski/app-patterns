{
  "name": "angular-modules-auth",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular/modules/auth/src",
  "prefix": "auth",
  "tags": ["scope:frontend"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
