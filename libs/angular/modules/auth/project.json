        "jestConfig": "libs/angular/modules/auth/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/angular/modules/auth/**/*.ts",
          "libs/angular/modules/auth/**/*.html"
        ]
      }
    }
  }
}
